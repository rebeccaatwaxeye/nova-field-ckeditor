{
    "name": "bayareawebpro/nova-field-ckeditor",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.0|^9.0",
        "spatie/laravel-image-optimizer": "^1.6"
    },
    "require-dev": {
        "laravel/nova": "~4.2"
    },
    "autoload": {
        "psr-4": {
            "BayAreaWebPro\\NovaFieldCkEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BayAreaWebPro\\NovaFieldCkEditor\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
